<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
    
    <bean id="CompetitionService"
        class="Competition.CompetitionService" />
    <bean id="CompetitionController"
        class="Competition.CompetitionController" />
    <bean id="SpringBootRestApplication"
        class="Competition.SpringBootRestApplication" />
    <bean id="User"
        class="Competition.User" />
    <bean id="Users"
        class="Competition.Users" />
 
</beans>